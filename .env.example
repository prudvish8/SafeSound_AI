# Example .env file for WhatsApp Telugu Chatbot Backend

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# Twilio Template SIDs
TWILIO_TEMPLATE_SID_EN_CONFIRM=your_english_confirmation_template_sid_here
TWILIO_TEMPLATE_SID_TE_CONFIRM=your_telugu_confirmation_template_sid_here
TWILIO_TEMPLATE_SID_TE_SCHEME_DETAILS=your_telugu_scheme_details_template_sid_here
TWILIO_TEMPLATE_SID_EN_SCHEME_DETAILS=your_english_scheme_details_template_sid_here

# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Whisper Configuration
WHISPER_REMOTE_URL=your_whisper_remote_url_here

# Database Configuration
DB_FILE=your_database_file_path_here

# Add any other environment variables as needed
